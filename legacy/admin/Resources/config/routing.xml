		<default key="_controller">AdminBundle:Spread:spreads</default>
	</route>
	<route id="admin/spread/detail" path="/cabinet/spread/detail/{id}">
		<default key="_controller">AdminBundle:Spread:spreadDetail</default>
	</route>

	<route id="admin/tasks" path="/cabinet/tasks">
		<default key="_controller">AdminBundle:Task:tasks</default>
	</route>
	
