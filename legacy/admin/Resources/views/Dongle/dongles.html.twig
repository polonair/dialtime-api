{% extends 'AdminBundle::page.html.twig' %}

{% block head_title %}AdminLTE 2 | Blank Page{% endblock %}

{% block content_header %}
	<h1>
		Управление донглами
		<small>it all starts here</small>
	</h1>
	<ol class="breadcrumb">
		<li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
		<li><a href="#">Examples</a></li>
		<li class="active">Blank page</li>
	</ol>
{% endblock %}

{% block content %}
	<div class="nav-tabs-custom">
		<ul class="nav nav-tabs">
			<li class="active"><a href=""><i class="fa fa-list"></i> Список донглов</a></li>
			<li class=""><a href="{{ path('admin/dongle/add') }}"><i class="fa fa-plus"></i> Добавить донгл</a></li>
		</ul>
		<div class="tab-content">
			<ul>
				{% for dongle in dongles %}
					<li>#{{ dongle.id }} {{ dongle.number }} </li>
				{% endfor %}
			</ul>
		</div>
	</div>
{% endblock %}
